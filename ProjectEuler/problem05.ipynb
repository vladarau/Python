    "def is_multiple(n):\n",
    "    for i in range (1, 21):\n",
    "        if (n % i != 0):\n",
    "            return False\n",
    "    return True    \n",
    "     \n",
    "k = 21\n",
    "while(1):\n",
    "    if(is_multiple(k)):\n",
    "        print(k)\n",
    "        break\n",
    "    k += 1    "
   ]
